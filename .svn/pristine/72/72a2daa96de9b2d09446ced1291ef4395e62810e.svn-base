package com.hotelsystem.bean;

public class MembersBean {
	private String m_id;
	private String m_name;
	private double m_consume;
	private int m_c_id;
	private int flag;
	public String getM_id() {
		return m_id;
	}
	public void setM_id(String m_id) {
		this.m_id = m_id;
	}
	public String getM_name() {
		return m_name;
	}
	public void setM_name(String m_name) {
		this.m_name = m_name;
	}
	public double getM_consume() {
		return m_consume;
	}
	public void setM_consume(double m_consume) {
		this.m_consume = m_consume;
	}
	public int getM_c_id() {
		return m_c_id;
	}
	public void setM_c_id(int m_c_id) {
		this.m_c_id = m_c_id;
	}
	public int getFlag() {
		return flag;
	}
	public void setFlag(int flag) {
		this.flag = flag;
	}
	
}
